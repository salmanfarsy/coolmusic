<%-include('../partials/header')%>
<link rel="stylesheet" href="/css/form.css">
<%-include('../partials/nav')%>
<h1>Add A new Song</h1>
<form action="/songs" method="POST" class="form">
<div class="box">
    <label for="title">Title</label>
    <input type="text" name="song[title]" required>
</div>
<div class="box">
    <label for="title">Image URL</label>
    <input type="text" name="song[image]" required>
</div>
<div class="box">
    <label for="title">Artist</label>
    <input type="text" name="song[artist]]" required>
</div>
<div class="box">
    <label for="title">Youtube Link</label>
    <input type="text" name="song[yt]" required>
</div>
<div class="box">
    <label for="title">Ratings</label>
    <input type="number" step="0.01" max="10" name="song[rating]" required>
</div>
<div class="box">
    <label for="title">Review</label>
    <textarea  name="song[review]" cols="30" rows="10" required></textarea>
</div>
<input type="submit">
</form>
    
    <%-include('../partials/footer')%>